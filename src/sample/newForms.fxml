  <HBox id="HBox" alignment="CENTER_LEFT" prefHeight="500.0" prefWidth="970.0" spacing="5.0" VBox.vgrow="NEVER">
    <padding>
      <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
    </padding>
    <WebView fx:id="webViewBotton" prefHeight="197.0" prefWidth="600.0" BorderPane.alignment="BOTTOM_LEFT" />
