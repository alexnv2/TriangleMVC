<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>

<VBox fx:id="vBox" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">

  <MenuBar>
    <Menu mnemonicParsing="false" text="Треугольники">

      <RadioMenuItem fx:id="menuOxygon" mnemonicParsing="false" onAction="#onClickOxygon" selected="true" text="Остроугольные" />
      <RadioMenuItem fx:id="menuObtuse" mnemonicParsing="false" onAction="#onClickObtuse" text="Тупоугольные" />
      <RadioMenuItem fx:id="menuRegular" mnemonicParsing="false" onAction="#onClickRegular" text="Прямоугольные" />
      <RadioMenuItem fx:id="menuIsosceles" mnemonicParsing="false" onAction="#onClickIsosceles" text="Равнобедренные" />
      <RadioMenuItem fx:id="menuEquilateral" mnemonicParsing="false" onAction="#onClickEquilateral" text="Равносторонние" />
      <SeparatorMenuItem mnemonicParsing="false" />
      <Menu mnemonicParsing="false" text="Признаки равенства">
        <MenuItem fx:id="menuEqualTr" mnemonicParsing="false" onAction="#onClickEquil" text="Первый признак равенства треугольников" />
        <MenuItem fx:id="menuSecondTr" mnemonicParsing="false" onAction="#onClickSecond" text="Второй признак равенства треугольников " />
        <MenuItem fx:id="menuTread" mnemonicParsing="false" onAction="#onClickTread" text="Третий признак равенства треугольников" />
      </Menu>
      <Menu mnemonicParsing="false" text="Признаки подобия">
        <MenuItem fx:id="menuEqualTrPod" mnemonicParsing="false" onAction="#onClickEquilPod" text="Первый признак подобия треугольников" />
        <MenuItem fx:id="menuSecondTrPod" mnemonicParsing="false" onAction="#onClickSecondPod" text="Второй признак подобия треугольников " />
        <MenuItem fx:id="menuTreadPod" mnemonicParsing="false" onAction="#onClickTreadPod" text="Третий признак подобия треугольников" />
      </Menu>
    </Menu>
    <Menu mnemonicParsing="false" text="Медианы">
      <CheckMenuItem fx:id="menuMedianaA" accelerator="Ctrl+N" mnemonicParsing="false" onAction="#onClickMedianaA" text="Из угла А" />
      <CheckMenuItem fx:id="menuMedianaB" accelerator="Ctrl+O" mnemonicParsing="false" onAction="#onClickMedianaB" text="Из угла В" />
      <CheckMenuItem fx:id="menuMedianaC" accelerator="Ctrl+R" mnemonicParsing="false" onAction="#onClickMedianaC" text="Из угла С" />
    </Menu>
    <Menu mnemonicParsing="false" text="Биссектрисы">
      <CheckMenuItem fx:id="menuBisectorA" mnemonicParsing="false" onAction="#onClickBisectorA" text="Из угла А" />
      <CheckMenuItem fx:id="menuBisectorB" mnemonicParsing="false" onAction="#onClickBisectorB" text="Из угла B" />
      <CheckMenuItem fx:id="menuBisectorC" mnemonicParsing="false" onAction="#onClickBisectorC" text="Из угла C" />
    </Menu>
    <Menu mnemonicParsing="false" text="Высоты">
      <CheckMenuItem fx:id="menuHigthA" mnemonicParsing="false" onAction="#onClickHigthA" text="Из угла А" />
      <CheckMenuItem fx:id="menuHigthB" mnemonicParsing="false" onAction="#onClickHigthB" text="Из угла B" />
      <CheckMenuItem fx:id="menuHigthC" mnemonicParsing="false" onAction="#onClickHigthC" text="Из угла C" />
    </Menu>
    <Menu mnemonicParsing="false" text="Серединные перпендикуляры">
      <CheckMenuItem fx:id="menuMiddlePerpendicularAB" mnemonicParsing="false" onAction="#onClickMiddlePerpendicularAB" text="К стороне АB" />
      <CheckMenuItem fx:id="menuMiddlePerpendicularBC" mnemonicParsing="false" onAction="#onClickMiddlePerpendicularBC" text="К стороне BC" />
      <CheckMenuItem fx:id="menuMiddlePerpendicularAC" mnemonicParsing="false" onAction="#onClickMiddlePerpendicularAC" text="К стороне AC" />
    </Menu>
    <Menu mnemonicParsing="false" text="Окружности">
      <CheckMenuItem fx:id="menuCircleIn" mnemonicParsing="false" onAction="#onClickCircleIn" text="Вписанная окружность" />
      <CheckMenuItem fx:id="menuCircleOut" mnemonicParsing="false" onAction="#onClickCircleOut" text="Описанная окружность" />
    </Menu>
    <Menu mnemonicParsing="false" text="О программе">
      <MenuItem fx:id="menuAbout" mnemonicParsing="false" onAction="#onAbout" text="О программе" />
    </Menu>
  </MenuBar>
  <SplitPane fx:id="splitPane" dividerPositions="0.2, 0.7" focusTraversable="true" VBox.vgrow="SOMETIMES">
    <AnchorPane>
      <WebView fx:id="webViewLeft" maxHeight="-1.0" maxWidth="-1.0" minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
    </AnchorPane>
    <StackPane fx:id="Cartesian">
      <Pane fx:id="paneGrid" />
      <Pane fx:id="paneShape" accessibleRole="NODE" onMouseDragged="#onMouseDraggen" onMousePressed="#onMousePressed" onScroll="#onScroll">
        <Arc fx:id="arcA" fill="TRANSPARENT" stroke="BLUE" strokeWidth="2" />
        <Arc fx:id="arcB" fill="TRANSPARENT" stroke="RED" strokeWidth="2" />
        <Arc fx:id="arcC" fill="TRANSPARENT" stroke="GREEN" strokeWidth="2" />
        <Circle fx:id="iC" fill="TRANSPARENT" stroke="GREEN" strokeWidth="2" visible="false" />
        <Circle fx:id="ic" fill="GREEN" radius="3" strokeWidth="2" visible="false" />
        <Circle fx:id="vc" fill="BLUE" radius="3" strokeWidth="2" visible="false" />
        <Circle fx:id="vC" fill="TRANSPARENT" stroke="Blue" strokeWidth="2" visible="false" />

        <Circle fx:id="A" fill="blue" onMouseDragged="#onMouseDraggen" onMouseEntered="#onMouseEntered" onMousePressed="#onMousePressed" onMouseReleased="#mouseReleased" radius="5" />
        <Circle fx:id="B" fill="RED" onMouseDragged="#onMouseDraggen" onMouseEntered="#onMouseEntered" onMousePressed="#onMousePressed" onMouseReleased="#mouseReleased" radius="5" />
        <Circle fx:id="C" fill="green" onMouseDragged="#onMouseDraggen" onMouseEntered="#onMouseEntered" onMousePressed="#onMousePressed" onMouseReleased="#mouseReleased" radius="5" />
        <Line fx:id="a" stroke="Black" strokeWidth="2.0" />
        <Line fx:id="b" stroke="Black" strokeWidth="2.0" />
        <Line fx:id="c" stroke="Black" strokeWidth="2.0" />

        <Circle fx:id="mA" fill="cornflowerblue" radius="3" visible="false" />
        <Line fx:id="ma" stroke="blue" visible="false" />
        <Circle fx:id="mB" fill="cornflowerblue" radius="3" visible="false" />
        <Line fx:id="mb" stroke="blue" visible="false" />
        <Circle fx:id="mC" fill="cornflowerblue" radius="3" visible="false" />
        <Line fx:id="mc" stroke="blue" visible="false" />

        <Circle fx:id="hA" fill="silver" radius="3" visible="false" />
        <Line fx:id="ha" stroke="darkred" visible="false" />
        <Circle fx:id="hB" fill="silver" radius="3" visible="false" />
        <Line fx:id="hb" stroke="darkred" visible="false" />
        <Circle fx:id="hC" fill="silver" radius="3" visible="false" />
        <Line fx:id="hc" stroke="darkred" visible="false" />

        <Circle fx:id="bA" fill="GREEN" radius="3" visible="false" />
        <Line fx:id="ba" stroke="seagreen" visible="false" />
        <Circle fx:id="bB" fill="GREEN" radius="3" visible="false" />
        <Line fx:id="bb" stroke="seagreen" visible="false" />
        <Circle fx:id="bC" fill="GREEN" radius="3" visible="false" />
        <Line fx:id="bic" stroke="seagreen" visible="false" />

        <Line fx:id="spa" stroke="royalblue" visible="false" />
        <Line fx:id="spb" stroke="royalblue" visible="false" />
        <Line fx:id="spc" stroke="royalblue" visible="false" />

        <Line fx:id="la" stroke="grey" visible="false" />
        <Line fx:id="lb" stroke="gray" visible="false" />
        <Line fx:id="lc" stroke="grey" visible="false" />

        <Line fx:id="la1" stroke="grey" visible="false" />
        <Line fx:id="lb1" stroke="gray" visible="false" />
        <Line fx:id="lc1" stroke="grey" visible="false" />

        <Line fx:id="angle90Ax" stroke="gray" strokeWidth="2" visible="false" />
        <Line fx:id="angle90Ay" stroke="grey" strokeWidth="2" visible="false" />
        <Line fx:id="angle90Bx" stroke="gray" strokeWidth="2" visible="false" />
        <Line fx:id="angle90By" stroke="grey" strokeWidth="2" visible="false" />
        <Line fx:id="angle90Cx" stroke="gray" strokeWidth="2" visible="false" />
        <Line fx:id="angle90Cy" stroke="grey" strokeWidth="2" visible="false" />
        <Text fx:id="poindA" strokeType="OUTSIDE" strokeWidth="0.0" text="A">
          <font>
            <Font size="18.0" />
          </font>
        </Text>
        <Text fx:id="poindB" strokeType="OUTSIDE" strokeWidth="0.0" text="B">
          <font>
            <Font size="18.0" />
          </font>
        </Text>
        <Text fx:id="poindC" strokeType="OUTSIDE" strokeWidth="0.0" text="C">
          <font>
            <Font name="Times New Roman" size="18.0" />
          </font>
        </Text>
        <Text fx:id="poindE" strokeType="OUTSIDE" strokeWidth="0.0" text="E" visible="false">
          <font>
            <Font name="Times New Roman" size="18.0" />
          </font>
        </Text>
        <Text fx:id="poindD" strokeType="OUTSIDE" strokeWidth="0.0" text="D" visible="false">
          <font>
            <Font name="Times New Roman" size="18.0" />
          </font>
        </Text>
        <Text fx:id="poindF" strokeType="OUTSIDE" strokeWidth="0.0" text="F" visible="false">
          <font>
            <Font name="Times New Roman" size="18.0" />
          </font>
        </Text>
        <Text fx:id="poindG" strokeType="OUTSIDE" strokeWidth="0.0" text="G" visible="false">
          <font>
            <Font name="Times New Roman" size="18.0" />
          </font>
        </Text>
        <Text fx:id="poindH" strokeType="OUTSIDE" strokeWidth="0.0" text="H" visible="false">
          <font>
            <Font name="Times New Roman" size="18.0" />
          </font>
        </Text>
        <Text fx:id="poindI" strokeType="OUTSIDE" strokeWidth="0.0" text="I" visible="false">
          <font>
            <Font name="Times New Roman" size="18.0" />
          </font>
        </Text>
        <Text fx:id="poindJ" strokeType="OUTSIDE" strokeWidth="0.0" text="J" visible="false">
          <font>
            <Font name="Times New Roman" size="18.0" />
          </font>
        </Text>
        <Text fx:id="poindK" strokeType="OUTSIDE" strokeWidth="0.0" text="K" visible="false">
          <font>
            <Font name="Times New Roman" size="18.0" />
          </font>
        </Text>
        <Text fx:id="poindL" strokeType="OUTSIDE" strokeWidth="0.0" text="L" visible="false">
          <font>
            <Font name="Times New Roman" size="18.0" />
          </font>
        </Text>
        <Text fx:id="poindO" strokeType="OUTSIDE" strokeWidth="0.0" text="O" visible="false">
          <font>
            <Font name="Times New Roman" size="18.0" />
          </font>
        </Text>
        <Text fx:id="poindS" strokeType="OUTSIDE" strokeWidth="0.0" text="S" visible="false">
          <font>
            <Font name="Times New Roman" size="18.0" />
          </font>
        </Text>
      </Pane>
    </StackPane>


      
    <VBox>
      <Accordion>
        <panes>
          <TitledPane animated="false" text="Вершины треугольника">
            <TableView fx:id="TableTreangle" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="vertex" minWidth="15.0" prefWidth="89.0" text="Углы треугольника" />
                <TableColumn prefWidth="68.0" text="Координаты">
                  <columns>
                    <TableColumn fx:id="coordinatesX" prefWidth="46.0" text="X" />
                    <TableColumn fx:id="coordinatesY" prefWidth="46.0" text="Y" />
                  </columns>
                </TableColumn>
                <TableColumn fx:id="angle" prefWidth="56.0" text="Угол гр." />
              </columns>
            </TableView>
          </TitledPane>
        </panes>
      </Accordion>
      <Accordion>
        <panes>
          <TitledPane animated="false" text="Длины треугольника">

            <TableView fx:id="LineTreangle">
              <columns>
                <TableColumn fx:id="LineS" prefWidth="120.0" text="Сторона" />
                <TableColumn fx:id="LineD" prefWidth="117.0" text="Длина, см" />
              </columns>
            </TableView>
        </TitledPane>
      </panes>
    </Accordion>
      <Accordion>
        <panes>
          <TitledPane animated="false" text="Дополнительно">
            <AnchorPane />
          </TitledPane>
        </panes>
      </Accordion>
    </VBox>


   </SplitPane>

  <HBox id="HBox" alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="900.0" spacing="5.0" >
    <padding>
      <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
    </padding>
    <AnchorPane >
    <WebView fx:id="webViewBotton"  prefHeight="100.0" prefWidth="900" />
    </AnchorPane>
  </HBox>
</VBox>
