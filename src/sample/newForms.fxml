            <font>
              <Font name="Times New Roman" size="18.0" />
            </font>
          </Text>
        </Pane>
      </StackPane>
      <VBox>
        <Accordion>
          <panes>
            <TitledPane animated="false" text="Вершины треугольника">
              <TableView fx:id="TableTreangle" prefHeight="200.0" prefWidth="161.0" BorderPane.alignment="CENTER">
                <columns>
                  <TableColumn fx:id="vertex" prefWidth="65.0" text="Углы треугольника" />
                  <TableColumn fx:id="coordinates" prefWidth="102.0" text="Координаты">
                    <columns>
                      <TableColumn fx:id="coordinatesX" prefWidth="46.0" text="X" />
                      <TableColumn fx:id="coordinatesY" prefWidth="46.0" text="Y" />
                    </columns>
                  </TableColumn>
                  <TableColumn fx:id="angle" prefWidth="56.0" text="Угол гр." />
                </columns>
              </TableView>
            </TitledPane>
          </panes>
        </Accordion>
        <Accordion>
          <panes>
            <TitledPane animated="false" text="Длины треугольника">

                        <TableView fx:id="LineTreangle" prefHeight="200.0" prefWidth="200.0">
                          <columns>
                            <TableColumn fx:id="LineS" prefWidth="95.0" text="Сторона" />
                            <TableColumn fx:id="LineD" prefWidth="55.0" text="Длина, см" />
                          </columns>
                        </TableView>
          </TitledPane>
        </panes>
      </Accordion>
        <Accordion>
          <panes>
            <TitledPane animated="false" text="Дополнительно">
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
