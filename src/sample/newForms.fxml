<VBox fx:id="vBox" prefHeight="600.0" prefWidth="970.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">

  <MenuBar>
    <Menu mnemonicParsing="false" text="Треугольники">

      <RadioMenuItem fx:id="menuOxygon" mnemonicParsing="false" onAction="#onClickOxygon" selected="true" text="Остроугольные" />
      <RadioMenuItem fx:id="menuObtuse" mnemonicParsing="false" onAction="#onClickObtuse" text="Тупоугольные" />
      <RadioMenuItem fx:id="menuRegular" mnemonicParsing="false" onAction="#onClickRegular" text="Прямоугольные" />
      <RadioMenuItem fx:id="menuIsosceles" mnemonicParsing="false" onAction="#onClickIsosceles" text="Равнобедренные" />
      <RadioMenuItem fx:id="menuEquilateral" mnemonicParsing="false" onAction="#onClickEquilateral" text="Равносторонние" />
      <SeparatorMenuItem mnemonicParsing="false" />
      <Menu mnemonicParsing="false" text="Признаки равенства">
        <items>
          <MenuItem fx:id="menuEqualTr" mnemonicParsing="false" onAction="#onClickEquil" text="Первый признак равенства треугольников" />
          <MenuItem fx:id="menuSecondTr" mnemonicParsing="false" onAction="#onClickSecond" text="Второй признак равенства треугольников " />
          <MenuItem fx:id="menuTread" mnemonicParsing="false" onAction="#onClickTread" text="Третий признак равенства треугольников" />
        </items>
      </Menu>
    </Menu>
    <Menu mnemonicParsing="false" text="Медианы">
      <CheckMenuItem fx:id="menuMedianaA" accelerator="Ctrl+N" mnemonicParsing="false" onAction="#onClickMedianaA" selected="false" text="Из угла А" />
      <CheckMenuItem fx:id="menuMedianaB" accelerator="Ctrl+O" mnemonicParsing="false" onAction="#onClickMedianaB" selected="false" text="Из угла В" />
      <CheckMenuItem fx:id="menuMedianaC" accelerator="Ctrl+R" mnemonicParsing="false" onAction="#onClickMedianaC" selected="false" text="Из угла С" />
    </Menu>
    <Menu mnemonicParsing="false" text="Биссектрисы">
      <CheckMenuItem fx:id="menuBisectorA" mnemonicParsing="false" onAction="#onClickBisectorA" selected="false" text="Из угла А" />
      <CheckMenuItem fx:id="menuBisectorB" mnemonicParsing="false" onAction="#onClickBisectorB" selected="false" text="Из угла B" />
      <CheckMenuItem fx:id="menuBisectorC" mnemonicParsing="false" onAction="#onClickBisectorC" selected="false" text="Из угла C" />
    </Menu>
    <Menu mnemonicParsing="false" text="Высоты">
      <CheckMenuItem fx:id="menuHigthA" mnemonicParsing="false" onAction="#onClickHigthA" selected="false" text="Из угла А" />
      <CheckMenuItem fx:id="menuHigthB" mnemonicParsing="false" onAction="#onClickHigthB" selected="false" text="Из угла B" />
      <CheckMenuItem fx:id="menuHigthC" mnemonicParsing="false" onAction="#onClickHigthC" selected="false" text="Из угла C" />
    </Menu>
    <Menu mnemonicParsing="false" text="Серединные перпендикуляры">
      <CheckMenuItem fx:id="menuMiddlePerpendicularAB" mnemonicParsing="false" onAction="#onClickMiddlePerpendicularAB" selected="false" text="К стороне АB" />
      <CheckMenuItem fx:id="menuMiddlePerpendicularBC" mnemonicParsing="false" onAction="#onClickMiddlePerpendicularBC" selected="false" text="К стороне BC" />
      <CheckMenuItem fx:id="menuMiddlePerpendicularAC" mnemonicParsing="false" onAction="#onClickMiddlePerpendicularAC" selected="false" text="К стороне AC" />
    </Menu>
    <Menu mnemonicParsing="false" text="Окружности">
      <CheckMenuItem fx:id="menuCircleIn" mnemonicParsing="false" onAction="#onClickCircleIn" selected="false" text="Вписанная окружность" />
      <CheckMenuItem fx:id="menuCircleOut" mnemonicParsing="false" onAction="#onClickCircleOut" selected="false" text="Описанная окружность" />
    </Menu>
  </MenuBar>
  <SplitPane fx:id="splitPane" dividerPositions="0.7, 0.4" focusTraversable="true" onDragDetected="#onClickSplit" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
    <items>
      <ScrollPane>
        <WebView fx:id="webViewLeft" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="480.0" />
            <opaqueInsets>
               <Insets />
            </opaqueInsets>
      </ScrollPane>
      <StackPane fx:id="st_pane" SplitPane.resizableWithParent="false">
        <Pane fx:id="setka" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="600.0" />
        <Pane fx:id="BP" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="600.0" BorderPane.alignment="CENTER">
          <Arc fx:id="arcA" fill="TRANSPARENT" onMouseEntered="#mouseEntered" stroke="BLUE" type="OPEN" />
          <Arc fx:id="arcB" fill="TRANSPARENT" onMouseEntered="#mouseEntered" stroke="RED" type="OPEN" />
          <Arc fx:id="arcC" fill="TRANSPARENT" onMouseEntered="#mouseEntered" stroke="GREEN" type="OPEN" />
          <Circle fx:id="iC" fill="TRANSPARENT" stroke="GREEN" strokeWidth="2" visible="false" />
          <Circle fx:id="ic" fill="GREEN" radius="3" strokeWidth="2" visible="false" />
          <Circle fx:id="vc" fill="BLUE" radius="3" strokeWidth="2" visible="false" />
          <Circle fx:id="vC" fill="TRANSPARENT" stroke="Blue" strokeWidth="2" visible="false" />

          <Circle fx:id="A" centerX="50" centerY="200" fill="blue" onMouseDragExited="#mouseDragExited" onMouseDragged="#mouseDraggen" onMouseEntered="#mouseEntered" onMousePressed="#mousePressed" onMouseReleased="#mouseReleased" radius="5" />
          <Circle fx:id="B" centerX="100" centerY="50" fill="RED" onMouseDragExited="#mouseDragExited" onMouseDragged="#mouseDraggen" onMouseEntered="#mouseEntered" onMousePressed="#mousePressed" onMouseReleased="#mouseReleased" radius="5" />
          <Circle fx:id="C" centerX="200" centerY="200" fill="green" onMouseDragExited="#mouseDragExited" onMouseDragged="#mouseDraggen" onMouseEntered="#mouseEntered" onMousePressed="#mousePressed" onMouseReleased="#mouseReleased" radius="5" />
          <Line fx:id="a" endX="200" endY="200" onMouseEntered="#mouseEntered" startX="100.0" startY="50.0" stroke="Black" strokeWidth="2.0" />
          <Line fx:id="b" endX="200" endY="200" onMouseEntered="#mouseEntered" startX="50" startY="200" stroke="Black" strokeWidth="2.0" />
          <Line fx:id="c" endX="100.0" endY="51.0" onMouseEntered="#mouseEntered" startX="50" startY="200" stroke="Black" strokeWidth="2.0" />

          <Circle fx:id="mA" fill="cornflowerblue" radius="3" visible="false" />
          <Line fx:id="ma" onMouseEntered="#mouseEntered" stroke="blue" visible="false" />
          <Circle fx:id="mB" fill="cornflowerblue" radius="3" visible="false" />
          <Line fx:id="mb" stroke="blue" visible="false" />
          <Circle fx:id="mC" fill="cornflowerblue" radius="3" visible="false" />
          <Line fx:id="mc" stroke="blue" visible="false" />

          <Circle fx:id="hA" fill="silver" radius="3" visible="false" />
          <Line fx:id="ha" stroke="darkred" visible="false" />
